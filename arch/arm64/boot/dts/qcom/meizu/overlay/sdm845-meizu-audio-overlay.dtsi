        "MIC BIAS2", "Headset Mic", 
        "AMIC3", "MIC BIAS3", 
        "MIC BIAS2", "ANCRight Headset Mic", 
        "AMIC4", "MIC BIAS2", 
        "MIC BIAS2", "ANCLeft Headset Mic", 
        "AMIC5", "MIC BIAS3", 
        "MIC BIAS3", "Handset Mic", 
        "DMIC0", "MIC BIAS1", 
        "MIC BIAS1", "Digital Mic0", 
        "DMIC1", "MIC BIAS1", 
        "MIC BIAS1", "Digital Mic1", 
