&qupv3_se0_spi {
    cs35l41_0: cs35l41@0 {
        #sound-dai-cells = <1>;
        compatible = "cirrus,cs35l41";
        spi-max-frequency = <12000000>;
        reg = <0x0>;
        interrupt-parent = <&tlmm>;
        interrupts = <11 0x08>;
        reset-gpios = <&tlmm 7 0x1>;
        VA-supply = <&pm8998_s4>;
        VP-supply = <&audio_vsys_regulator>;
        cirrus,boost-peak-milliamp = <3500>;
        cirrus,boost-ind-nanohenry = <1000>;
        cirrus,boost-cap-microfarad = <15>;
        cirrus,audio-channel = <9>;
        cirrus,advisory-channel = <9>;

        cirrus,gpio-config2 {
            cirrus,gpio-src-select = <4>;
            cirrus,gpio-output-enable;
        };
    };

    cs35l41_1: cs35l41@1 {
        #sound-dai-cells = <1>;
        compatible = "cirrus,cs35l41";
        spi-max-frequency = <12000000>;
        reg = <0x1>;
        interrupt-parent = <&tlmm>;
        interrupts = <30 0x08>;
        reset-gpios = <&tlmm 7 0x1>;
        VA-supply = <&pm8998_s4>;
        VP-supply = <&audio_vsys_regulator>;
        cirrus,boost-peak-milliamp = <3500>;
        cirrus,boost-ind-nanohenry = <1000>;
        cirrus,boost-cap-microfarad = <15>;
        cirrus,stereo-config;
        cirrus,audio-channel = <8>;
        cirrus,advisory-channel = <8>;

        cirrus,gpio-config2 {
            cirrus,gpio-src-select = <4>;
            cirrus,gpio-output-enable;
        };
    };
};

